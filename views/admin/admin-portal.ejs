<%- include('../partials/head.ejs')%>

    <body class="bg-gray-100 min-h-screen">
        <%- include('../partials/navbar.ejs')%>


            <main class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
                <!-- Welcome Section -->
                <div class="mb-8">
                    <h1 class="text-3xl font-bold text-gray-900">Welcome back, Admin!</h1>
                    <p class="text-gray-600 mt-2">Here's what's happening with your properties today.</p>
                </div>

                <!-- Stats Grid -->
                <div class="grid grid-cols-1 md:grid-cols-3 gap-6 mb-8">
                    <!-- Registered Properties Card -->
                    <div
                        class="bg-red-500 rounded-xl p-6 text-white shadow-lg transform hover:scale-105 transition-transform duration-200">
                        <div class="flex items-center justify-between">
                            <div>
                                <p class="text-blue-100 text-sm font-medium">Registered Properties</p>
                                <p class="text-3xl font-bold mt-2">
                                    <%= registerHomes.length%>
                                </p>
                                <p class="text-blue-100 text-sm mt-1">+3 this month</p>
                            </div>
                            <div class="bg-white bg-opacity-20 rounded-lg p-3">
                                <i data-lucide="building" class="h-8 w-8"></i>
                            </div>
                        </div>
                    </div>

                    <!-- Total Bookings Card -->
                    <div
                        class="bg-red-500 rounded-xl p-6 text-white shadow-lg transform hover:scale-105 transition-transform duration-200">
                        <div class="flex items-center justify-between">
                            <div>
                                <p class="text-pink-100 text-sm font-medium">Total Bookings</p>
                                <p class="text-3xl font-bold mt-2">187</p>
                                <p class="text-pink-100 text-sm mt-1">+12 this week</p>
                            </div>
                            <div class="bg-white bg-opacity-20 rounded-lg p-3">
                                <i data-lucide="calendar-check" class="h-8 w-8"></i>
                            </div>
                        </div>
                    </div>

                    <!-- Total Earnings Card -->
                    <div
                        class="bg-red-500 rounded-xl p-6 text-white shadow-lg transform hover:scale-105 transition-transform duration-200">
                        <div class="flex items-center justify-between">
                            <div>
                                <p class="text-cyan-100 text-sm font-medium">Total Earnings</p>
                                <p class="text-3xl font-bold mt-2">$45,760</p>
                                <p class="text-cyan-100 text-sm mt-1">+8.2% from last month</p>
                            </div>
                            <div class="bg-white bg-opacity-20 rounded-lg p-3">
                                <i data-lucide="dollar-sign" class="h-8 w-8"></i>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Main Content Grid -->
                <div class="grid grid-cols-1 lg:grid-cols-3 gap-8">
                    <!-- Profile Section -->
                    <div class="lg:col-span-2">
                        <div class="bg-white rounded-xl shadow-sm border border-gray-200 overflow-hidden">
                            <div class="profile-card px-6 py-8 text-white">
                                <div class="flex items-center justify-between">
                                    <div class="flex items-center">
                                        <div class="bg-white bg-opacity-20 rounded-full ">
                                            <i data-lucide="user" class="h-12 w-12"></i>
                                        </div>
                                        <div class="flex items-center">
                                            <img src="data:image/jpeg;base64,"
                                                alt="Admin Profile"
                                                class="w-16 h-16 rounded-full object-cover border-2 border-white shadow mr-4">
                                            <div>
                                                <h2 class="text-2xl font-bold text-black">Admin Profile</h2>
                                                <p class="text-black">Manage your account details</p>
                                            </div>
                                        </div>
                                    </div>
                                    <a
                                        class="cursor-pointer text-center bg-red-500 bg-opacity-20 hover:bg-opacity-30 rounded-lg px-8 py-2 flex items-center transition-colors">

                                        <span id="edit-btn-text">Edit</span>
                                    </a>
                                </div>
                            </div>

                            <div class="p-6">
                                <div id="profile-view" class="space-y-4">
                                    <div class="grid grid-cols-1 md:grid-cols-2 gap-4">
                                        <div>
                                            <label class="block text-sm font-medium text-gray-700 mb-1">Full
                                                Name</label>
                                            <p class="text-gray-900 font-semibold" id="display-name">John Anderson</p>
                                        </div>
                                        <div>
                                            <label class="block text-sm font-medium text-gray-700 mb-1">Role</label>
                                            <p class="text-gray-900 font-semibold">Property Manager</p>
                                        </div>
                                        <div>
                                            <label class="block text-sm font-medium text-gray-700 mb-1">Email</label>
                                            <p class="text-gray-900 font-semibold" id="display-email">
                                                john.anderson@propmanager.com
                                            </p>
                                        </div>
                                        <div>
                                            <label class="block text-sm font-medium text-gray-700 mb-1">Phone</label>
                                            <p class="text-gray-900 font-semibold" id="display-phone">+1 (555) 123-4567
                                            </p>
                                        </div>
                                        <div>
                                            <label class="block text-sm font-medium text-gray-700 mb-1">Joined
                                                Date</label>
                                            <p class="text-gray-900 font-semibold">January 2023</p>
                                        </div>
                                        <div>
                                            <label class="block text-sm font-medium text-gray-700 mb-1">Status</label>
                                            <span
                                                class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-green-100 text-green-800">Active</span>
                                        </div>
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>

                    <!-- Action Buttons Section -->
                    <div class="space-y-6">
                        <!-- Quick Actions Card -->
                        <div class="bg-white rounded-xl shadow-sm border border-gray-200 p-6">
                            <h3 class="text-lg font-semibold text-gray-900 mb-4 flex items-center">
                                <i data-lucide="zap" class="h-5 w-5 mr-2 text-yellow-600"></i>
                                Quick Actions
                            </h3>
                            <div class="space-y-3">
                                <a href="/admin-homes"
                                    class="cursor-pointer w-full bg-gradient-to-r from-blue-600 to-blue-700 hover:from-blue-700 hover:to-blue-800 text-white px-6 py-3 rounded-lg font-medium transition-all duration-200 transform hover:scale-105 flex items-center justify-center">
                                    <i data-lucide="eye" class="h-5 w-5 mr-2"></i>
                                    View Registered Properties
                                </a>
                                <a href="/register-home"
                                    class="cursor-pointer w-full bg-gradient-to-r from-green-600 to-green-700 hover:from-green-700 hover:to-green-800 text-white px-6 py-3 rounded-lg font-medium transition-all duration-200 transform hover:scale-105 flex items-center justify-center">
                                    Register New Property
                                </a>
                            </div>
                        </div>



                        <!-- Recent Activity Card -->
                        <div class="bg-white rounded-xl shadow-sm border border-gray-200 p-6">
                            <h3 class="text-lg font-semibold text-gray-900 mb-4 flex items-center">
                                <i data-lucide="activity" class="h-5 w-5 mr-2 text-purple-600"></i>
                                Recent Activity
                            </h3>
                            <div class="space-y-3">
                                <div class="flex items-center p-3 bg-gray-50 rounded-lg">
                                    <div class="bg-green-100 rounded-full p-2 mr-3">
                                        <i data-lucide="check" class="h-4 w-4 text-green-600"></i>
                                    </div>
                                    <div class="flex-1">
                                        <p class="text-sm font-medium text-gray-900">New booking received</p>
                                        <p class="text-xs text-gray-500">2 hours ago</p>
                                    </div>
                                </div>
                                <div class="flex items-center p-3 bg-gray-50 rounded-lg">
                                    <div class="bg-blue-100 rounded-full p-2 mr-3">
                                        <i data-lucide="home" class="h-4 w-4 text-blue-600"></i>
                                    </div>
                                    <div class="flex-1">
                                        <p class="text-sm font-medium text-gray-900">Property verified</p>
                                        <p class="text-xs text-gray-500">5 hours ago</p>
                                    </div>
                                </div>
                                <div class="flex items-center p-3 bg-gray-50 rounded-lg">
                                    <div class="bg-yellow-100 rounded-full p-2 mr-3">
                                        <i data-lucide="dollar-sign" class="h-4 w-4 text-yellow-600"></i>
                                    </div>
                                    <div class="flex-1">
                                        <p class="text-sm font-medium text-gray-900">Payment received</p>
                                        <p class="text-xs text-gray-500">1 day ago</p>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
            </main>












    </body>



    </html>